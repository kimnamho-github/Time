<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.home.Time">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.DIAL_PHONE" />
    <uses-permission
        android:name="android.permission.PACKAGE_USAGE_STATS"
        tools:ignore="ProtectedPermissions" />

    <application
        android:name="com.example.Time.Parse_App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ParseLogin">
        <activity
            android:name="com.example.Time.RecordGradeGraphActivity"
            android:exported="false" />
        <activity
            android:name="com.example.Time.now_behavior"
            android:exported="false" />
        <activity
            android:name="com.example.Time.InspectionGradeGraphActivity"
            android:exported="false" />
        <activity
            android:name="com.example.Time.SumGradeActivity"
            android:exported="false" />
        <activity
            android:name="com.example.Time.ResultActivity"
            android:exported="false" />
        <activity
            android:name="com.example.Time.TextActivity"
            android:exported="false" />
        <activity
            android:name="com.example.Time.Record_Activity"
            android:exported="false" />
        <activity
            android:name="com.example.Time.Usage_Activity"
            android:exported="false" />
        <activity
            android:name="com.example.Time.FirstActivity"
            android:exported="false" />
        <activity
            android:name="com.example.Time.SecondActivity"
            android:exported="false" />
        <activity
            android:name="com.example.Time.ThirdActivity"
            android:exported="false" />
        <activity
            android:name="com.example.Time.ForthActivity"
            android:exported="false" />
        <activity
            android:name="com.example.Time.AddressApiActivity"
            android:exported="false" />
        <activity
            android:name="com.example.Time.HomeActivity"
            android:exported="true" />
        <activity
            android:name="com.example.Time.ResetPasswordActivity"
            android:exported="false" />
        <activity
            android:name="com.example.Time.SignupActivity"
            android:exported="false" />
        <activity
            android:name="com.example.Time.LoginActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="com.parse.SERVER_URL"
            android:value="https://parseapi.back4app.com" />
        <meta-data
            android:name="com.parse.APPLICATION_ID"
            android:value="@string/back4app_app_id" />
        <meta-data
            android:name="com.parse.CLIENT_KEY"
            android:value="@string/back4app_client_key" />
    </application>

</manifest>